{
  "name": "learn.react",
  "version": "1.0.0",
  "description": "this repository for personal testing some features react.js",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./dist/index.html --open",
    "srt": "webpack",
    "build": "rm -rf dist && parcel build ./dist/index.html --public-url ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
