  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.0",
    "@babel/preset-react": "7.16.0",
    "@linthtml/linthtml": "^0.8.6",
    "@mate-academy/linthtml-config": "^0.0.2",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
